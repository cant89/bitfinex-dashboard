{
  "name": "bitfinex",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Davide Cantelli <cant1989@gmail.com>",
  "license": "ISC",
  "homepage": "",
  "scripts": {
    "lint": "eslint 'src/**/*.ts?(x)' && tsc",
    "start": "webpack-dev-server --config=webpack/webpack.local.config.js --color --progress",
    "build:prod": "webpack --config webpack/webpack.prod.config.js --progress --colors",
    "build:beta": "webpack --config webpack/webpack.beta.config.js --progress --colors",
    "build:stage": "webpack --config webpack/webpack.stage.config.js --progress --colors",
    "analyze": "NODE_ENV=BUNDLE_ANALYZER webpack-dev-server --config=webpack/webpack.local.config.js --color --progress",
    "test": "jest --verbose --watchAll",
    "test:once": "jest --verbose",
    "test:once:coverage": "jest -i --verbose --coverage",
    "check-types": "tsc"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.1.5",
    "@types/recompose": "^0.30.6",
    "@types/styled-components": "^5.1.2",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "connected-react-router": "6.3.1",
    "history": "^4.9.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5",
    "redux-saga-routines": "^3.1.3",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.1",
    "typesafe-actions": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-conventional": "^8.0.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^3.3.8",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/gitlab": "^4.0.4",
    "@semantic-release/gitlab-config": "^5.0.0",
    "@svgr/webpack": "^5.4.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.12",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.1.7",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga-routines": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-loader": "^8.0.5",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "imagemin-lint-staged": "^0.4.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.0.2",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.32",
    "typescript": "^3.7.4",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "tsc && lint-staged",
      "pre-push": "npm run lint && npm run test:once:coverage"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(png|jpg|gif|svg)$": "<rootDir>/helpers/imageMock.js",
      "^#/(.*)": "<rootDir>/src/$1"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "notify": true,
    "resetMocks": true,
    "coveragePathIgnorePatterns": [
      "__tests__",
      "coverage/",
      "src/static",
      "webpack/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**"
    ],
    "globals": {
      "__TEST__": true,
      "API_BASE_URL": {}
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "verifyConditions": [
      "@semantic-release/git",
      "@semantic-release/changelog"
    ],
    "extends": "@semantic-release/gitlab-config",
    "prepare": [
      {
        "path": "@semantic-release/exec",
        "cmd": "npm version ${nextRelease.version} --git-tag-version=false"
      },
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "publish": [],
    "success": [],
    "fail": [],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "breaking": true,
              "release": "major"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "minor"
            },
            {
              "type": "perf",
              "release": "minor"
            },
            {
              "type": "refactor",
              "release": "minor"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ]
    ]
  }
}
